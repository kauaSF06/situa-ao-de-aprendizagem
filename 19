programa {
  funcao inicio() {
    inteiro idade
    escreva("escreva sua idade\n")
    leia(idade)
    se((idade>=18) e (idade<70)){
      escreva("voto obrigatorio")
    }
      senao se((idade>=16)e(idade<=17) ou (idade>=70))
      {
        escreva("voto facultativo")
      }senao
      escreva("nao pode votar")
  }
}
