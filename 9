programa {
  funcao inicio() {
    real valor, desconto, valortotal
    escreva("escreva o valor da sua compra: ")
    leia(valor)
    desconto = (valor*0.15)
    se(valor>=200){
      valortotal = valor - desconto
      escreva("o valor do produto e : ", valortotal)
      
    }
    senao 
    escreva(valor)
  }
}
