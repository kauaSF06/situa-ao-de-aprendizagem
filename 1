programa {
  funcao inicio() {
    inteiro   qpao, qbroa
    real pao, broa, resultadob, resultadop
    pao = 0.75
    broa = 1.50
    escreva("digite a quantidade de paes: ")
    leia(qpao)
    escreva(" digite a quantidade de broas: ")
    leia(qbroa)
    resultadop = qpao * pao
    resultadob = qbroa * broa
    escreva("quanto ficou o pao: \n", resultadop,"\n")
    escreva("quanto ficou a quantidade broa: ", resultadob,"\n")
    escreva("quanto ficou o total: ", resultadob * resultadop)


  }
}
